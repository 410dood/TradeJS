{
  "name": "tradejs-desktop",
  "description": "TradeJS desktop wrapper",
  "main": "main",
  "scripts": {
    "postinstall": "electron-rebuild",
    "start": "electron ../",
    "build": "node packager",
    "build:windows": "node packager --platform=win32",
    "build:mac": "node packager --platform=darwin",
    "build:linux": "node packager --platform=linux"
  },
  "dependencies": {
    "electron": "1.6.2",
    "electron-prebuilt-compile": "^1.6.2",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "cross-env": "^4.0.0",
    "debug": "^2.6.1",
    "electron": "1.6.2",
    "electron-builder": "16.6.1",
    "electron-packager": "8.6.0",
    "electron-prebuilt": "1.4.13",
    "electron-rebuild": "^1.5.7"
  },
  "author": "Kewin Brandsma",
  "private": false
}
