{
  "name": "tradejs-desktop",
  "description": "TradeJS desktop wrapper",
  "version": "0.0.1",
  "main": "main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron ../",
    "build": "build --config=./electron/config.json --projectDir=../",
    "build:all": "build -mwl --config=./electron/config.json --projectDir=../",
    "build:windows": "build -w --config=./electron/config.json --projectDir=../",
    "build:mac": "build -m --config=./electron/config.json --projectDir=../",
    "build:linux": "build -l --config=./electron/config.json --projectDir=../"
  },
  "devDependencies": {
    "cross-env": "4.0.0",
    "debug": "2.6.6",
    "electron": "1.6.6",
    "electron-builder": "17.3.1",
    "electron-prebuilt": "1.4.13",
    "electron-prebuilt-compile": "1.6.6",
    "electron-rebuild": "1.5.7",
    "mkdirp": "0.5.1"
  },
  "author": "Kewin Brandsma",
  "private": false,
  "dependencies": {
    "node-gyp": "3.6.1"
  }
}
