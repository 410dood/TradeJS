{
  "name": "tradejs-server",
  "version": "0.0.11",
  "description": "Javascript Trading Client",
  "license": "Apache License 2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/DutchKevv/TradeJS.git"
  },
  "keywords": [
    "TradeJS",
    "Stock",
    "Forex",
    "Trade",
    "Trading",
    "Node",
    "Trading",
    "NodeJS"
  ],
  "scripts": {
    "lint": "gulp tslint",
    "dist": "build",
    "dev": "cross-env DEBUG=TradeJS:* gulp server:dev --web",
    "build": "gulp server:build",
    "prod": "cross-env NODE_ENV=production node dist/server/bin/www.js",
    "_server:run": "tsc -p ./tsconfig.json | cross-env DEBUG=TradeJS* nodemon bin/www.js --web --delay 3",
    "_server:run2": "tsc -p ./server && concurrently \"tsc -w -p ./server\" \"DEBUG node dist/server/bin/www.js --delay 1\" ",
    "_server:dist": "cross-env DEBUG=TradeJS* node dist/server/bin/www.js",
    "_server:build": "tsc -p ./server"
  },
  "author": "Kewin Brandsma",
  "private": false,
  "dependencies": {
    "@types/node": "^7.0.5",
    "backbone-events-standalone": "0.2.7",
    "body-parser": "1.16.1",
    "cli-table": "0.3.1",
    "cors": "2.8.1",
    "cross-env": "3.1.4",
    "debug": "2.6.1",
    "deepmerge": "1.3.2",
    "directory-tree": "1.2.0",
    "event-stream": "*",
    "express": "4.14.1",
    "freeport": "1.0.5",
    "gulp": "3.9.1",
    "gulp-sourcemaps": "2.4.1",
    "gulp-tslint": "^7.1.0",
    "gulp-typescript": "3.1.5",
    "lodash": "4.17.4",
    "minimist": "^1.2.0",
    "mkdirp": "0.5.1",
    "moment": "*",
    "node-ipc": "git://github.com/RIAEvangelist/node-ipc.git#v9.x",
    "nodemon": "^1.11.0",
    "readable-timespan": "1.2.0",
    "request": "^2.79.0",
    "rmdir": "^1.2.0",
    "run-sequence": "1.2.2",
    "socket.io": "1.7.3",
    "source-map-support": "0.4.11",
    "sqlite3": "3.1.8",
    "sqlite3-transactions": "0.0.5",
    "tree-kill": "1.1.0",
    "tslint": "^4.4.2",
    "typescript": "^2.1.6"
  },
