{
  "name": "TradeJS",
  "version": "0.0.4",
  "description": "Javascript Trading Client",
  "main": "index.js",
  "license": "Apache License 2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/DutchKevv/TradeJS.git"
  },
  "keywords": [
    "TradeJS",
    "Stock",
    "Forex",
    "Trade",
    "Trading",
    "Node",
    "Trading",
    "NodeJS"
  ],
  "scripts": {
    "postinstall": "electron-rebuild",
    "dist": "build",
    "client:dev": "cd ./src/client && npm run dev",
    "client:prod": "gulp client:prod",
    "server:dev": "DEBUG=TradeJS* gulp server:dev --web",
    "server:build": "gulp server:build",
    "server:prod": "NODE_ENV=production node dist/server/bin/www.js",
    "electron:build": "electron-packager ./ TradeJS --platform=darwin --overwrite --ignore=/e2e --ignore=/src/client",
    "electron:dev": "npm run _electron",
    "electron:prod": "npm run build && NODE_ENV=production npm run _electron",
    "start": "ng serve --proxy-config proxy.conf.json & npm run server:dev",
    "lint": "tslint \"client/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "_electron": "electron --js-flags=\"--harmony_async_await\" ./ --electron=true",
    "prod": "NODE_ENV=production node dist/server/bin/www.js",
    "build": "ng build --prod --output-path=dist/client && npm run _server:build",
    "_server:run": "tsc -p ./src/server && tsc -w -p ./src/server & DEBUG=TradeJS* nodemon dist/server/bin/www.js --delay 1 -q",
    "_server:run2": "tsc -p ./server && concurrently \"tsc -w -p ./server\" \"DEBUG node dist/server/bin/www.js --delay 1\" ",
    "_server:dist": "DEBUG=TradeJS* node dist/server/bin/www.js",
    "_server:build": "tsc -p ./server"
  },
  "author": "Kewin Brandsma",
  "private": false,
  "dependencies": {
    "backbone-events-standalone": "^0.2.7",
    "body-parser": "~1.15.2",
    "cli-table": "^0.3.1",
    "codelyzer": "2.0.0-beta.1",
    "compression": "^1.6.2",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.1",
    "cors": "^2.8.1",
    "debug": "^2.3.0",
    "deepmerge": "^1.3.0",
    "directory-tree": "^1.1.1",
    "event-stream": "^3.3.4",
    "express": "^4.13.4",
    "express-jwt": "^5.1.0",
    "freeport": "^1.0.5",
    "gulp": "^3.9.1",
    "gulp-sourcemaps": "^2.2.0",
    "gulp-typescript": "^3.1.2",
    "jsonwebtoken": "^7.1.9",
    "lodash": "^4.16.6",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "morgan": "^1.7.0",
    "node-ipc": "git://github.com/RIAEvangelist/node-ipc.git#v8.x",
    "node-uuid": "^1.4.7",
    "readable-timespan": "^1.1.0",
    "run-sequence": "^1.2.2",
    "socket.io": "^1.5.1",
    "source-map-support": "^0.4.6",
    "sqlite3": "^3.1.8",
    "ts-helpers": "^1.1.1",
