{
  "name": "TradeJS",
  "version": "0.0.8",
  "description": "Javascript Trading Client",
  "main": "index.js",
  "license": "Apache License 2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/DutchKevv/TradeJS.git"
  },
  "keywords": [
    "TradeJS",
    "Stock",
    "Forex",
    "Trade",
    "Trading",
    "Node",
    "Trading",
    "NodeJS"
  ],
  "scripts": {
    "postinstall": "electron-rebuild",
    "dist": "build",
    "client:dev": "cd ./src/client && npm run dev",
    "client:prod": "gulp client:prod",
    "server:dev": "DEBUG=TradeJS* gulp server:dev --web",
    "server:build": "gulp server:build",
    "server:prod": "NODE_ENV=production node dist/server/bin/www.js",
    "electron:build": "electron-packager ./ TradeJS --platform=darwin --overwrite --ignore=/e2e --ignore=/src/client",
    "electron:rebuild": "electron-rebuild ./ TradeJS --platform=darwin --overwrite --ignore=/e2e --ignore=/src/client -f -w sqlite3",
    "electron:dev": "npm run _electron",
    "electron:prod": "npm run build && NODE_ENV=production npm run _electron",
    "start": "ng serve --proxy-config proxy.conf.json & npm run server:dev",
    "lint": "tslint \"client/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "_electron": "electron --js-flags=\"--harmony_async_await\" ./ --electron=true",
    "prod": "NODE_ENV=production node dist/server/bin/www.js",
    "build": "ng build --prod --output-path=dist/client && npm run _server:build",
    "_server:run": "tsc -p ./src/server && tsc -w -p ./src/server & DEBUG=TradeJS* nodemon dist/server/bin/www.js --delay 1 -q",
    "_server:run2": "tsc -p ./server && concurrently \"tsc -w -p ./server\" \"DEBUG node dist/server/bin/www.js --delay 1\" ",
    "_server:dist": "DEBUG=TradeJS* node dist/server/bin/www.js",
    "_server:build": "tsc -p ./server"
  },
  "author": "Kewin Brandsma",
  "private": false,
  "dependencies": {
    "@angular/core": "2.4.1",
    "backbone-events-standalone": "0.2.7",
    "body-parser": "1.15.2",
    "cli-table": "0.3.1",
    "codelyzer": "2.0.0-beta.4",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "core-js": "2.4.1",
    "cors": "2.8.1",
    "debug": "2.6.0",
    "deepmerge": "1.3.1",
    "directory-tree": "1.2.0",
    "event-stream": "3.3.4",
    "express": "4.14.0",
    "express-jwt": "5.1.0",
    "freeport": "1.0.5",
    "gulp": "3.9.1",
    "gulp-sourcemaps": "2.2.0",
    "gulp-typescript": "3.1.4",
    "jsonwebtoken": "7.2.1",
    "lodash": "4.17.4",
    "minimist": "1.2.0",
    "mkdirp": "0.5.1",
    "morgan": "1.7.0",
    "node-ipc": "git://github.com/RIAEvangelist/node-ipc.git#v8.x",
    "node-uuid": "1.4.7",
    "readable-timespan": "1.2.0",
    "run-sequence": "1.2.2",
    "socket.io": "1.7.2",
    "source-map-support": "0.4.8",
    "sqlite3": "3.1.8",
    "ts-helpers": "1.1.2",
    "ts-loader": "1.3.3",
    "ts-node": "2.0.0",
    "tslint": "4.3.1",
    "typescript": "2.1.4",
    "underscore": "1.8.3",
    "webpack-stream": "3.2.0",
    "zone.js": "0.7.4"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.33",
    "@types/compression": "0.0.33",
    "@types/core-js": "0.9.35",
    "@types/cors": "2.8.0",
    "@types/debug": "0.0.29",
    "@types/express": "4.0.34",
    "@types/freeport": "1.0.20",
    "@types/jasmine": "2.5.40",
    "@types/jsonwebtoken": "7.2.0",
    "@types/lodash": "4.14.46",
    "@types/minimist": "1.2.0",
    "@types/mkdirp": "0.3.29",
    "@types/moment": "2.13.0",
    "@types/node": "6.0.58",
    "@types/node-uuid": "0.0.28",
    "@types/socket.io": "1.4.27",
    "@types/socket.io-client": "1.4.29",
    "@types/sqlite3": "2.2.31",
    "electron": "1.4.13",
    "electron-builder": "10.17.3",
    "electron-packager": "8.4.0",
    "electron-prebuilt": "1.4.13",
    "electron-rebuild": "1.5.5",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.0.0",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-jasmine": "1.1.0",
    "karma-remap-istanbul": "0.4.0",
    "nodemon": "1.11.0",
    "protractor": "4.0.14",
    "write-file-webpack-plugin": "3.4.2"
  },
  "directories": {
    "app": "./",
    "out": "/Users/kewinbrandsma/Desktop",
    "buildResources": "./"
  },
  "build": {
    "npmRebuild": false,
    "productName": "TradeJS",
    "appId": "tradejs",
    "mac": {
      "category": "your.app.category.type"
    },
    "win": {
      "iconUrl": "(windows-only) https link to icon"
    },
    "files": [
      "dist/**/*",
      "config/**/*",
      "src/**/*",
      "!src/client${/*}",
      "_npm_module_backup",
      "doc/**/*",
      "gulpfile.js",
      "readmy.md",
      "tsconfig.json",
      "tslint.json",
      "index.js"
    ]
  }
}
