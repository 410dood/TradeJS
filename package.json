  "directories": {
    "app": "./",
    "out": "/Users/kewinbrandsma/Desktop",
    "buildResources": "./"
  },
  "build": {
    "npmRebuild": false,
    "productName": "TradeJS",
    "appId": "tradejs",
    "mac": {
      "category": "your.app.category.type"
    },
    "win": {
      "iconUrl": "(windows-only) https link to icon"
    },
    "files": [
      "dist/**/*",
      "config/**/*",
      "src/**/*",
      "!src/client${/*}",
      "_npm_module_backup",
      "doc/**/*",
      "gulpfile.js",
      "readmy.md",
      "tsconfig.json",
      "tslint.json",
      "index.js"
    ]
  }
