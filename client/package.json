{
  "name": "coinpush",
  "displayName": "App",
  "version": "1.0.0",
  "description": "CoinPush client",
  "repository": {
    "type": "git",
    "url": "https://github.com/DutchKevv/TradeJS.git"
  },
  "main": "index.js",
  "scripts": {
    "start": "ng build --watch --aot ---env=development -output-hashing none",
    "build": "ng build --prod --aot --env=production --output-hashing none",
    "build-stats": "ng build --prod --aot --env=production --stats-json --output-hashing none",
    "build-verbose": "ng build --prod --env=production --stats-json --output-hashing none --verbose > build.txt",
    "load-symbol-images": "node ./tools/load-symbol-images.js",
    "build-spritesheet": "node ./tools/build-spritesheet.js",
    "bundle-report": "webpack-bundle-analyzer www/stats.json"
  },
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "dependencies": {
    "@angular/cli": "^6.0.0-beta.4",
    "@angular/common": "^6.0.0-beta.6",
    "@angular/compiler": "^6.0.0-beta.6",
    "@angular/compiler-cli": "^6.0.0-beta.6",
    "@angular/core": "^6.0.0-beta.6",
    "@angular/forms": "^6.0.0-beta.6",
    "@angular/platform-browser": "^6.0.0-beta.6",
    "@angular/router": "^6.0.0-beta.6",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0",
    "@ngrx/core": "1.2.0",
    "@types/node": "^9.4.6",
    "bootstrap": "^4.0.0",
    "highcharts": "^6.0.7",
    "localforage": "^1.5.7",
    "socket.io-client": "2.0.4",
    "typescript": "~2.6.2",
    "zone.js": "^0.8.20",
    "webpack-bundle-analyzer": "2.10.1"
  }
}
