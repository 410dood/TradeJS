{
  "name": "com.example.app",
  "displayName": "App",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "start": "npm run build-spritesheet && ng build --watch --aot --no-sourcemaps",
    "build": "npm run build-spritesheet && ng build --prod --aot --env=prod",
    "build-verbose": "ng build --prod --env=prod --verbose > build.txt",
    "build-mobile": "cordova build --no-inc",
    "build-android": "cordova build android --no-inc",
    "build-ios": "cordova build ios --no-inc",
    "build-mobile-release": "cordova build --release --inc-version",
    "build-android-release": "cordova build android --release --inc-version",
    "build-ios-release": "cordova build ios --release --inc-version",
    "emulate-android": "cordova emulate android",
    "emulate-ios": "cordova emulate ios",
    "run-android": "cordova run android",
    "run-ios": "cordova run android",
    "load-symbol-images": "node ./tools/load-symbol-images.js",
    "build-spritesheet": "node ./tools/build-spritesheet.js"
  },
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "dependencies": {
    "@angular/cli": "^1.6.2",
    "@angular/common": "^5.1.2",
    "@angular/compiler": "^5.1.2",
    "@angular/compiler-cli": "^5.1.2",
    "@angular/core": "^5.1.2",
    "@angular/forms": "^5.1.2",
    "@angular/http": "^5.1.2",
    "@angular/language-service": "^5.1.2",
    "@angular/platform-browser": "^5.1.2",
    "@angular/router": "^5.1.2",
    "@ngrx/core": "1.2.0",
    "@ngrx/store": "^4.1.1",
    "@types/ace": "0.0.36",
    "@types/jquery": "^3.2.17",
    "@types/lodash": "^4.14.91",
    "@types/node": "^8.5.2",
    "android-versions": "^1.2.1",
    "bootstrap": "^4.0.0-beta.2",
    "cordova-android": "^7.0.0",
    "cordova-browser": "^5.0.2",
    "cordova-build-increment": "^0.1.0",
    "cordova-plugin-admobpro": "^2.29.29",
    "cordova-plugin-app-version": "^0.1.9",
    "cordova-plugin-extension": "^1.5.3",
    "cordova-plugin-fileopener": "^1.0.5",
    "cordova-plugin-firebase": "^0.1.25",
    "cordova-plugin-secure-storage": "^2.6.8",
    "cordova-plugin-splashscreen": "^5.0.0",
    "cordova-plugin-statusbar": "^2.4.0",
    "cordova-plugin-whitelist": "^1.3.3",
    "core-js": "^2.5.3",
    "deepmerge": "^2.0.1",
    "highcharts": "^6.0.4",
    "jquery": "3.2.1",
    "jquery-resizable-dom": "0.20.0",
    "jquery-touchswipe": "1.6.18",
    "sharp": "^0.18.4",
    "socket.io-client": "2.0.4",
    "spritesheet-js": "^1.2.6",
    "typescript": "^2.6.2",
    "zone.js": "^0.8.18"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-admobpro": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-firebase": {},
      "cordova-plugin-app-version": {},
      "cordova-plugin-fileopener": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-secure-storage": {}
    },
    "platforms": [
      "browser",
      "android"
    ]
  }
}