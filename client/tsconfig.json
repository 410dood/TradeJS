{
  "compilerOptions": {
    "baseUrl": "./src",
    "allowJs": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": false,
    "lib": [
      "es2017",
      "dom"
    ],
    "sourceMap": false,
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "paths": {
      "@angular/*": [
        "../node_modules/@angular/*"
      ]
    }
  },
  "include": [
    "../shared/modules/coinpush/constant/index.ts",
    "../shared/modules/coinpush/util/countries.ts",
    "../shared/modules/coinpush/interface/notification.interface.ts",
    "./**/*.ts"
  ],
  "angularCompilerOptions": {
    "genDir": "./aot",
    "comments": false,
    "entryModule": "./app/app.module#AppModule",
    "preserveWhitespaces": false,
    "skipMetadataEmit": true
  }
}