{
  "project": {
    "name": "tradejs"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "www",
      "assets": [
        "firebase-messaging-sw.js",
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "app",
      "scripts": [
        "./assets/vendor/js/highcharts/highstock.js",
        "./assets/vendor/js/highcharts/modules/exporting.js",
        "./assets/vendor/js/highcharts/indicators/indicators.js",
        "./assets/vendor/js/highcharts/indicators/ema.js",
        "./assets/vendor/js/highcharts/indicators/bollinger-bands.js",
        "./assets/vendor/js/highcharts/indicators/atr.js",
        "./assets/vendor/js/highcharts/indicators/rsi.js",
        "./assets/custom/js/highcharts/highstock.theme.dark.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "warnings": {
    "typescriptMismatch": false
  }
}
