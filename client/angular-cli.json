{
  "project": {
    "version": "1.0.0-beta.22",
    "name": "tmp"
  },
  "apps": [
    {
      "root": "",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "app",
      "mobile": false,
      "styles": [
        "node_modules/font-awesome/css/font-awesome.css",
        "node_modules/bootstrap/dist/css/bootstrap.min.css",
        "node_modules/jstree/dist/themes/default/style.css",
        "node_modules/jstree/dist/themes/default-dark/style.css",

        "assets/custom/css/contextMenu.css",

        "app/style/main.scss",
        "app/style/variables/_variables.scss",
        "app/style/helpers/spinner.scss",
        "app/style/helpers/spinner-button.scss",
        "app/style/helpers/three-column.scss",
        "app/style/bootstrap-overwrite.scss"
      ],
      "scripts": [
        "assets/custom/js/electron.js",
        "assets/custom/js/contextMenu.js",
        "node_modules/bootstrap/dist/js/bootstrap.min.js",
        "node_modules/jquery-resizable-dom/dist/jquery-resizable",
        "node_modules/jstree/dist/jstree.js",
        "node_modules/jquery-touchswipe/jquery.touchSwipe.js",
        "assets/vendor/js/ace/ace.js",
        "assets/vendor/js/ace/theme-tomorrow_night_bright.js",
        "assets/vendor/js/ace/mode-javascript.js",
        "assets/vendor/js/ace/mode-typescript.js"
      ],
      "addons": [
        "node_modules/font-awesome/fonts/*.+(otf|eot|svg|ttf|woff|woff2)"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "source": "environments/environment.ts",
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "lint": [
    {
      "project": "./tsconfig.app.json",
      "exclude": ["**/node_modules/**/*", "./assets/**/*"]
    }
  ],
  "addons": [],
  "defaults": {
    "styleExt": "scss",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": true
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
